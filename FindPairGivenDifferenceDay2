5th problem on day 2
Find pair difference 
If the pair with the given difference is present return 1 else -1
class Solution {
  public:
    int findPair(int n, int x, vector<int> &nums) {
       unordered_map<int,int>mp;
      
      int ans=0;
          for(int i=0;i<n;i++){
          if(mp.find(nums[i]-x)!=mp.end()){
              ans++;
          } if(mp.find(nums[i]+x)!=mp.end()){
              ans++;
          }
   mp[nums[i]]++;
       }
       return ans==0?-1:1;
    }
};
