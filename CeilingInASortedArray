Found it easily 
class Solution {
  public:
    int findFloor(vector<long long> &v, long long n, long long x) {
        if(v[0]>x){
            return -1;
        }
        int ans;
        for(int i=0;i<n;i++){
            if(v[i]<=x){
             ans=i;
            }
        }return ans;
        
        // Your code here
    }
};
