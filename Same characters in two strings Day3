to count the number of same characters in both the strings
counted by using tolower()
class Solution{
    public:
    int sameChar(string A, string B)
    {
        int ans=0;
       for(int i=0;i<A.size();i++){
           if(tolower(A[i])==tolower(B[i]))
           {
               ans++;
           }
       }
       return ans;
    }
};
