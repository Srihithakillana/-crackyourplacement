Cycle in a linked list
fast and slow are used fast is moved twice where slow once then they meet if there is cycle
class Solution{
    public:
    bool hasCycle(ListNode *head){
        if(head==nullptr){
            return false;
        }
        ListNode *fast=head;
        ListNode *slow=head;
        while(fast&&fast->next){
            fast=fast->next->next;
            slow=slow->next;
            if(fast==slow)
            return true;
        }
        return false;
    }
};
